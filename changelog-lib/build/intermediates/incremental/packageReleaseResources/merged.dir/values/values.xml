<?xml version="1.0" encoding="utf-8"?>
<resources>
    <item name="changelog" type="raw"/>
    <string name="changelog_bug_improvement">[font color="#009933"][b]New: [/b][/font]</string>
    <string name="changelog_bug_prefix">[font color="#FF0000"][b]Bug: [/b][/font]</string>
    <string name="changelog_bullet" translatable="false">\u2022\u00A0</string>
    <string name="changelog_dialog_button">Ok</string>
    <string name="changelog_dialog_rate">Rate app</string>
    <string name="changelog_dialog_title">Changelog %1$s</string>
    <string name="changelog_show_more">Show more</string>
    <string name="changelog_version_title">%1$s</string>
    <style name="ChangelogDialogDarkTheme" parent="@style/Theme.MaterialComponents.Dialog">
        <item name="android:windowNoTitle">false</item>
    </style>
    <style name="ChangelogDialogLightTheme" parent="@style/Theme.MaterialComponents.Light.Dialog">
        <item name="android:windowNoTitle">false</item>
    </style>
    <style name="ChangelogHeader">
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="ChangelogHeaderDate" parent="ChangelogHeader">
        <item name="android:textAppearance">@android:style/TextAppearance.Small</item>
    </style>
    <style name="ChangelogHeaderVersion" parent="ChangelogHeader">
    </style>
    <style name="ChangelogMore" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_gravity">right|end</item>
        <item name="android:foreground">?selectableItemBackground</item>
    </style>
    <style name="ChangelogRow">
        <item name="android:textAppearance">@android:style/TextAppearance.Small</item>
    </style>
</resources>